<h1>Listing tag_images</h1>

<table>
  <tr>
    <th>Image</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tag_images.each do |tag_image| %>
  <tr>
    <td><%= tag_image.image %></td>
    <td><%= link_to 'Show', tag_image %></td>
    <td><%= link_to 'Edit', edit_tag_image_path(tag_image) %></td>
    <td><%= link_to 'Destroy', tag_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

